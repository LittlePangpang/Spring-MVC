<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-4.3.xsd"> 
	<!-- 将指定类配置给Spring，让Spring创建其对象的实例 -->
        <!-- 构造注入 -->
        <bean id="stu1" class="com/s1626022055/ioc/Student ">
         	<constructor-arg index="0" value="1615062096" ></constructor-arg>
         	<constructor-arg index="1" value="刘施阳1" ></constructor-arg>
         	<constructor-arg index="2" value="2019" ></constructor-arg>
         </bean>
         <!-- 设值注入 -->
         <bean id="stu2" class="com/s1626022055/ioc/Student ">
			<property name="no" value="6666666666" ></property>
         	<property name="name" value="刘施阳2" ></property>
         	<property name="age" value="2019" ></property>
         </bean>
 	
</beans>
